<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config cleartextTrafficPermitted="false">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
    <domain-config>
        <domain includeSubdomains="true">vattenfall.fi</domain>
        <domain includeSubdomains="true">elenia.fi</domain>
        <domain includeSubdomains="true">aws.elenia.fi</domain>
        <domain includeSubdomains="true">amazonaws.com</domain>
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </domain-config>
</network-security-config>